# Commands
dice-name = dice
dice-description = Roll some dice

dice-min-value-name = min_value
dice-max-value-name = max_value
dice-min-value-description = The minimum value for the dice. Default value is { $default }
dice-max-value-description = The maximum value for the dice. Default value is { $default }

# Errors
dice-invalid-min-max-value = min_value cannot be greater than max_value

# Success
dice-rolled = { $name }  rolled { $value }
